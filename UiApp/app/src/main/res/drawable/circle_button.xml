<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="4dp"
        android:right="4dp"
        android:left="4dp"
        android:bottom="4dp">
        <shape
            android:shape="oval">
            <solid android:color="#A8A8A8"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:dashGap="2dp"
                android:color="#6F6F6F"/>
        </shape>
    </item>
</layer-list>